<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oro.mergesdk"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.oro.mergesdk.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.oro.mergesdk.Oro_MergeLoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/zytx_Dialog">
        </activity>
        <activity
            android:name="com.oro.mergesdk.Oro_TwinkleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize">
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
        <meta-data
            android:name="GameCode"
            android:value="0" /><!-- 母包测试专用不需要修改 -->
        <meta-data
            android:name="ChannelId"
            android:value="0"/><!-- 母包测试专用不需要修改 -->
        
    </application>

</manifest>
